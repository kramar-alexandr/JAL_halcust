external updating procedure SendExportFileToServer(string);globalupdating procedure ForceGenCompMn(record RCVc RepSpec)begin  record SMFVc SMFr;  Boolean TrHs;    TrHs = true;  SMFr.StatusFlag = 1;  while (LoopKey("StatusFlag",SMFr,1,TrHs)) begin    if (SMFr.StatusFlag!=1) then begin      TrHs = false;    end else begin      if (nonblank(SMFr.ServerCode) and nonblank(SMFr.CompNr)) then begin        NewTimedTask("asdf" & SMFr.SMFCode,"asdf" & SMFr.SMFCode,"SendExportFileToServer",SMFr.SMFCode & "," & SMFr.ServerCode,CurrentDate,AddSeconds(CurrentTime,1),"","");      end;    end;  end;  return;end;