globalupdating procedure RemoveLostAttachmentsMn(record RCVc RepSpec)begin  Longint cnt,i;  string 255 fn,tstr,dir;  record Attach2Vc Attach2r;  UUID t_uuid;    dir = "Attach/";  cnt = CountFilesInDir(dir);  for (i=cnt-1;i>=0;i=i-1) begin    fn = GetFileNameInDir(dir,i);    tstr = dir & fn;    t_uuid = StringToUUID(fn);    Attach2r.UUID = t_uuid;    if (ReadFirstKey("UUID",Attach2r,1,true)==false) then begin      Delete_File(tstr);         end;  end;    return;end;globalupdating procedure ListLostAttachmentsRn(record RCVc RepSpec)begin  Longint cnt,i;  string 255 fn,tstr,dir;  record Attach2Vc Attach2r;  UUID t_uuid;    StartReportJob("Lost Attachments");    EndHeader;    dir = "Attach/";  cnt = CountFilesInDir(dir);  //LogText(0,cnt);  for (i=cnt-1;i>=0;i=i-1) begin    fn = GetFileNameInDir(dir,i);    tstr = dir & fn;    Attach2r.UUID = StringToUUID(fn);    if (ReadFirstKey("UUID",Attach2r,1,true)==false) then begin      StartFormat(15);      OutString(0,0,fn,false);      //OutString(200,0,Attach2r.FileSize,true);      EndFormat;        //LogText(0,"Left " & i & " out of " & cnt);      end;  end;    EndJob;    return;end;