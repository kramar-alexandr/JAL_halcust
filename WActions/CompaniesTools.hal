//dunnoremote procedure GetCompanyRec(var record GlobalUserVc);globalprocedure JALAddServerCompsm()begin  record GlobalUserVc GlobalUserr;  record CompaniesBlock Compr,Comp2r;  row CompaniesBlock Comprw,Comp2rw;  row GlobalUserVc GlobalUserrw;  string 20 host,port;  integer compnr,rw,rwc;  integer wn;  wn = CurWIndow;  rw = WindowActiveRow(wn);    DeselectWindow(wn,false);  GetWindowRecord(wn,Compr);  MatRowGet(Compr,0,Comprw);  host = Comprw.TCPIP;  port = Comprw.Port;  if rw > -1 then begin    MatRowGet(Compr,rw,Comprw);    GlobalUserr.Name = Comprw.ShortName;  end;  GetCompanyRec(GlobalUserr);  rwc = MatRowCnt(GlobalUserr);  for (rw = 0; rw < rwc; rw = rw + 1) begin    MatRowGet(GlobalUserr,rw,GlobalUserrw);    Comprw.CompCode = globalUserrw.CompCode;    Comprw.CompName = GlobalUserrw.CompName;    Comprw.ShortName = GlobalUserrw.AccessGroup;    Comprw.TCPIP = host;    Comprw.Port = port;    MatRowPut(Compr,MatRowCnt(Compr),Comprw);  end;  PutWindowRecord(wn,Compr);return;end;