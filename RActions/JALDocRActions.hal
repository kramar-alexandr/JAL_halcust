global updating function longint JALFileVcRecordUpdateAfter(record JALFileVc JFr,record JALFileVc JF2r,longint a, longint b)begin  longint res;  record JALFileHistVc JFHr;  if (JFr.Status <> JF2r.Status) then begin    RecordNew(JFHr);    JFHr.SerNr = NextSerNr("JALFileHistVc",CurrentDate,-1,false,"");    JFHr.DocSerNr = JFr.SerNr;    JFHr.TransDate = CurrentDate;    JFHr.TransTime = CurrentTime;    JFHr.Status = JFr.Status;    if recordStore(JFHr,true) then begin end;  end;  JALFileVcRecordUpdateAfter = res;  return;end;global updating function longint JALFileVcRecordSaveAfter(record JALFileVc JFr,record JALFileVc JF2r,longint a, longint b)begin  longint res;  record JALFileHistVc JFHr;  RecordNew(JFHr);  JFHr.SerNr = NextSerNr("JALFileHistVc",CurrentDate,-1,false,"");  JFHr.DocSerNr = JFr.SerNr;  JFHr.TransDate = CurrentDate;  JFHr.TransTime = CurrentTime;  JFHr.Status = JFr.Status;  if recordStore(JFHr,true) then begin end;  JALFileVcRecordSaveAfter = res;  return;end;