external function string 255 ToolWebNGTranslateText(integer);globalprocedure WebShowLoginName()begin  record CUVc CUr;  record UserVc USr;  //stopalert("a" & CurrentCust & "a  a" & CurrentUser & "a  adsf");  CUr.Code = CurrentCust;  if CurrentCust <> "" then begin    CUr.Code = CurrentCust;    if readfirstmain(CUr,1,true) then begin      WebOutStringFormat("<div style=""float:left"">" & ToolWebNGTranslateText(23021) & CUr.Name & "</div>");    //end else begin	  //WebOutStringFormat("<h2>" & ToolWebNGTranslateText(24164) & "</h2>");    end;  end else begin    if CurrentUser <> "" then begin	  USr.Code = CurrentUser;	  if readfirstmain(USr,1,true) then begin        WebOutStringFormat("<div style=""float:left"">" & ToolWebNGTranslateText(23021) & USr.Code & "</div>");      //end else begin        //WebOutStringFormat("<h2>" & ToolWebNGTranslateText(24164) & "</h2>");      end;	end;  end;  return;end;